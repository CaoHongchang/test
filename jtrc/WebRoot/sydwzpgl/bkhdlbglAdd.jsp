<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>

<html>
<head>
<title>报考活动列表新增</title>

<body>

		<form id="fm" method="post">
	<input type="hidden" id="id" name="id"/>
    <div id="aa" class="easyui-accordion" style="height:350px;">   
       <div title="" data-options="iconCls:'icon-help',fit:true,selected:true,collapsible:false" style="overflow:auto;padding:10px;">    
              <table class="table2">
      <tr>
      <td class="left">报考活动编码</td>
      <td class="right">
       <input class="easyui-textbox" id ="pno" name="pno" size="60" data-options="editable:false" style="height:22px">
	  </td>
      
      
      </tr>
      <tr>
      <td class="left">报考活动名称</td>
      <td class="right">
       <input class="easyui-textbox" id ="pname" name="pname" size="60" data-options="required:true" style="height:22px">
	   </td>
      </tr>
      
      <tr>
      <td class="left">前端可见</td>
      <td class="right">
       <select id ="remark" name="remark" class="easyui-combobox" data-options="required:true" style="width: 200px">
        <option value="是">是</option>   
        <option value="否">否</option>
        </select>
	   </td>
      </tr>

      </table>  
      </div>   
   
       </div>

      

             
    </form> 

</body>

</html>
