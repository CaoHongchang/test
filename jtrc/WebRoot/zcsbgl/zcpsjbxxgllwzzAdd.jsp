<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>

<html>
<head>
<title>论文专著</title>

<body>

	<form id="fm" method="post">
   
    <table class="table3">
      <tr>
      <td class="left">论文题目</td>
      <td class="right"><input id ="zhmc1" class="easyui-textbox" size="30"  data-options="required:true" style="height: 32px;"></td>
      </tr>
      <tr>
      <td class="left">刊出交流时间</td>
      <td class="right" style=""><input class="easyui-datetimebox" size="30" name="birthday"   style="height: 32px;"  data-options="required:true,showSeconds:false,editable:false"  > </td>
      </tr>
      <tr>
      <td class="left">刊出交流情况（刊物及会议名称、刊号等）</td>
      <td class="right"><input class="easyui-textbox"   style="height: 32px;" size="30" data-options="required:true" ></td>
      </tr> 
       <tr>
      <td class="left">备注</td>
      <td class="right"  ><input class="easyui-textbox" style="height: 60px;" size="30" data-options="required:true"></td>
      </tr>
      <tr>
      <td class="left">是否作为送审代表作 </td>
      <td class="right" style=""><input type="checkbox"></td>
      </tr>
     <tr>
      <td class="left">上传论文</td>
      <td class="right"  >
        
      <textarea name="aboutusDescription" style="height: 150px;" rows="10" id="aboutusDescription" style="width:90%;">
		</textarea>
      </td>
     
      </tr> 
      
      </table>  
      
      
       

    </form> 

</body>

</html>
