<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>

<html>
<head>
<title>学习经历</title>

<body>

	<form id="xxjlAddForm" method="post">
	 <input name="activityId" id="activityId" type="hidden">
      <table class="table2" style="margin:35px 0px">
      <tr>
      <td class="left">毕业院校</td>
      <td class="right"><input  name="school_name" class="easyui-textbox" size="20"  data-options="required:true" style="height:32px"></td>
      <td class="left">入学日期</td>
      <td class="right"><input  name="in_date" class="easyui-datebox"  size="20" data-options="editable:false" style="height:32px"></td>
      <td class="left">毕业时间</td>
      <td class="right"><input  name="graduation_date" class="easyui-datebox"  size="20" data-options="required:true,editable:false" style="height:32px"></td>
      </tr>
      <tr>
      <td class="left">学历</td>
      <td class="right">	<select id="education" name="education" class="easyui-combobox"  style="width:132px" data-options="required:true">
										<option value="初中">初中</option>
										<option value="高中">高中</option>
										<option value="专业">专业</option>
										<option value="本科">本科</option>
										<option value="研究生">研究生</option>
										<option value="博士">博士</option>
										<option value="博士后">博士后</option>
										</select>
										<!-- <input class="easyui-textbox"  name="education" size="20" data-options="required:true" style="height:32px"> --></td>
       <td class="left">学位</td>
      <td class="right"><input class="easyui-textbox"  name="degree" size="20"  style="height:32px"></td>
       <td class="left">证书</td>
      <td class="right"><input class="easyui-textbox" name="certificate"  size="20" data-options="required:true"  style="height:32px"></td>
      </tr>
      <tr>
       <td class="left">专业</td>
       <td class="right"><input class="easyui-textbox" name="pro_name"  size="20" data-options="required:true"  style="height:32px"></td>
       <td class="left">学制</td>
      <td class="right"><input class="easyui-textbox" name="xz" size="20"   style="height:32px"></td>
       <td class="left">取得方式</td>
       <td class="right"><input class="easyui-textbox" name="get_method"  size="20"  style="height:32px"></td>
      </tr>
    
      </table>  
       
    </form> 

</body>

</html>
