<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>
<%@ include file="/system/common.jsp" %>
<html>
<head>
<title>校园招聘会修改</title>
</head>
<body>
	<form id="fm" method="post">
	<input type="hidden" name="id" id="id1" /> 
   <div id="aa" class="easyui-accordion" >  
         <table class="table2">
		      <tr>
			      <td class="left">招聘会名称</td>
			      <td ><input id ="name1" name="name" class="easyui-textbox" data-options="required:true,
			      	validType:['length[0,20]']
			      " size="70" style="height:22px"></td>
			  </tr>
			  <tr>
			      <td class="left">举办时间</td>
			      <td ><input id ="hold_time" name="hold_time" class="easyui-datebox" 
			      data-options="required:true"  size="70" style="height:22px"></td>
		      </tr>
		      <tr>
			      <td class="left">举办地点</td>
			      <td ><input class="easyui-textbox" data-options="required:true,validType:['length[0,50]']" 
			      id ="hold_place" name="hold_place"  size="70" style="height:22px"></td>
			  </tr>
			  <tr>
		      	  <td class="left" colspan="2" style="text-align:left;padding-left:20px;">招聘会内容</td>
		      </tr>
		      <tr>
		      	  <td colspan="2">
		      	  	<textarea name="content" cols="" rows="10" id="content" class="span1-2" style="width:90%;"></textarea>
		      	  </td>
		      </tr>
      	</table>  
   </div> 

    </form> 
    <script type="text/javascript">
	$(function(){
		
	
	});
	</script>
</body>
</html>
