<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>
<%@ include file="/system/common.jsp" %>

<html>
<head>
   <title>业务查看</title>
</head>

<body>
	<form id="fmView" method="post">
		<div>
			<table class="table2" id="viewTable">
				<tr>
					<td class="left">
						发起时间：
					</td>
					<td class="right">
					    <input name="addDate" type="text" readonly="readonly"/>
					</td>
					<td class="left">
						发起人：
					</td>
					<td class="right">
					    <input name="userName" type="text" readonly="readonly"/>
					</td>
				</tr>
				<tr>
					<td class="left">
						身份证号：
					</td>
					<td class="right">
					    <input name="pnum" type="text" readonly="readonly"/>
					</td>
					<td class="left">
						手机号码：
					</td>
					<td class="right">
					    <input name="tel" type="text" readonly="readonly"/>
					</td>
				</tr>
				<tr>
					<td class="left">
						业务分类：
					</td>
					<td class="right">
					    <input name="typeName" type="text" readonly="readonly"/>
					</td>
					<td class="left">
						业务名称：
					</td>
					<td class="right">
					    <input name="businessName" type="text" readonly="readonly"/>
					</td>
				</tr>
				<tr>
					<td class="left">
						快递公司：
					</td>
					<td class="right">
					    <input name="kCompany" type="text" readonly="readonly"/>
					</td>
					<td class="left">
						快递单号：
					</td>
					<td class="right">
					    <input name="kpno" type="text" readonly="readonly"/>
					</td>
				</tr>
				<tr>
					<td class="left">
						寄出时间：
					</td>
					<td class="right">
					    <input name="kDate" type="text" readonly="readonly"/>
					</td>
				</tr>
				<tr>
                    <td class="left">其他说明：</td>
                    <td class="right" colspan="3">
                        <input name="remark" class="easyui-textbox" data-options="multiline:true" readonly="readonly" disabled="disabled" style="width:100%;height:100px">
                    </td>
                 </tr>
			</table>
		</div>
	</form> 
</body>

</html>

