    where bausi.enterprise_place_id = #{activityPlaceId,jdbcType=INTEGER} GROUP BY bausi.zkh  ORDER BY score+0 desc
  </select>
  
  <!-- 获取可见岗位列表 -->
  <select id="activityPlaceList" resultType="java.util.HashMap" parameterType="java.util.HashMap">
    SELECT bepi.id AS id, bai.pname AS hdmc, ei.`name` AS dwmc, pi.`name` AS gwmc,pi.`pno` AS gwdm,bai.pno as pno
	FROM bg_enterprise_place_info bepi 
	INNER JOIN bg_activity_enterprise bae ON bepi.activity_enterprise_id = bae.id
	INNER JOIN bg_activity_info bai ON bae.activity_id = bai.id
	INNER JOIN enterprise_info ei ON bae.enterprise_id = ei.id
	INNER JOIN place_info pi ON bepi.place_id = pi.id where 1=1 
		<if test="activity_id!=null and activity_id!='' and activity_id!=0 and activity_id!='0'">
         and bai.id = #{activity_id,jdbcType=INTEGER}
      	</if> 
      	<if test="enterprise_id!=null and enterprise_id!=''">
         and ei.id = #{enterprise_id,jdbcType=INTEGER}
         </if>
        <if test="gwmc!=null and gwmc!=''">
         and pi.name like CONCAT('%',#{gwmc,jdbcType=VARCHAR},'%')
        </if>
      	<if test="list!=null and list!=''">
         and ei.id in
           <foreach item="item" index="index" collection="list" open="("
            separator="," close=")">
            #{item}  
         </foreach> 
      </if> 
  </select>
  
