  <!-- 查询某岗位成绩情况 -->
  <select id="userScoreList" parameterType="java.util.HashMap" resultType="java.util.HashMap">
    select bausi.id, bausi.activity_user_id,bausi.order_id,bausi.name,baui.zkzh,bausi.card_number,bausi.score,bausi.remark
from bg_activity_user_score_info bausi
