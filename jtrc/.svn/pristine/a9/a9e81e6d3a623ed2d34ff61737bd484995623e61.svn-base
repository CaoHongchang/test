<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>
<%@ include file="/system/common.jsp" %>
 
<html>
<head>
<title>工作简历修改</title>

<body>

<form id="editWorkInfoFm" method="post">
<input type="hidden" name="da_dpno" id="da_dpno"/>
   <div>
   <table class="table2">
      <tr>
      <td class="left">起始时间</td>
      <td class="right">
     <input class="easyui-datebox" name="startDate" id="edit_start_date" size="20" data-options="showSeconds:false" value="" style="height:22px">
	</td>
      <td class="left">终止时间</td>
      <td class="right">
      <input class="easyui-datebox" name="endDate" id="edit_end_date" size="20" data-options="showSeconds:false" value="" style="height:22px">
      </td>
      </tr>
      <tr>
      <td class="left">所在单位</td>
      <td class="right">
      <input class="easyui-textbox"  name="enterpriseId" id="edit_enterprise_id" size="20"  style="height:22px">
       <td class="left">所在部门</td>
      <td class="right"><input class="easyui-textbox" name="departmentId" id="edit_department_id" size="20"  style="height:22px"></td>
      </tr>
      <tr>
      <td class="left">岗位名称</td>
      <td class="right"><input class="easyui-textbox" name="placeId" id="edit_place_id" size="20"   style="height:22px"></td>
     <td class="left"></td>
      <td class="right">
      
      </td>
      </tr>
         
       <tr>
      <td class="left">工作描述</td>
      <td class="right" colspan="4"> <input class="easyui-textbox" name="workName"  id="workName"  size="20" data-options="required:false,multiline:true" style="height:60px;width: 430px"></td>
      </tr>
      </table>  
      <font color="gray" style="margin-left: 120px">工作描述限制在150字之内</font> 
      </div>   
    </form> 

</body>

</html>
