		a.kpno,
		a.parent_id,
		a.remark,
		a.k_date,
		a.k_company,
		a.sider,
		a.pic1,
		a.pic2,
		a.pic3,
		b.`name` AS businessName,
		c.`name` AS typeName,
		d.`name` AS userName,
		d.pnum,
		a.other_name AS otherName,
		d.tel
	FROM
		rs_business_info a
		left join type_info b on a.business_id = b.id
		left join type_info c on a.parent_id = c.id
		inner join user_info d on a.uid = d.id
	WHERE  a.parent_id &gt; 0 
	<if test="userName != null">
        AND d.name like '%${userName}%'
    </if>
    <if test="sstate != null">
        AND a.sstate = #{sstate}
    </if>
    <if test="businessName != null">
        AND b.name like '%${businessName}%'
    </if>
  </select>
  
