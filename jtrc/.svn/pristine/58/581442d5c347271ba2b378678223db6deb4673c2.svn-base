package com.i84.earnpoint.model;

import java.io.Serializable;
import java.util.List;

/**
 * @author liu_quan
 *
 */
@SuppressWarnings("serial")
public class Code implements Serializable{
	private String typeInfoId;//信息ID
	private String parentInfoId;//父级信息ID
	private String typeId;//分类ID
	private String typeInfoCode;//编码
	private String typeInfoName;//名称
	private String remark;//备注
	
	private List<Code> children;//树控件子节点

	public String getTypeInfoId() {
		return typeInfoId;
	}
	
	public String getTypeId() {
		return typeId;
	}

	public String getParentInfoId() {
		return parentInfoId;
	}

	public void setParentInfoId(String parentInfoId) {
		this.parentInfoId = parentInfoId;
	}

	public void setTypeId(String typeId) {
		this.typeId = typeId;
	}

	public String getTypeInfoCode() {
		return typeInfoCode;
	}

	public void setTypeInfoCode(String typeInfoCode) {
		this.typeInfoCode = typeInfoCode;
	}

	public String getTypeInfoName() {
		return typeInfoName;
	}

	public void setTypeInfoName(String typeInfoName) {
		this.typeInfoName = typeInfoName;
	}

	public String getRemark() {
		return remark;
	}

	public void setRemark(String remark) {
		this.remark = remark;
	}

	public void setTypeInfoId(String typeInfoId) {
		this.typeInfoId = typeInfoId;
	}
	
	public List<Code> getChildren() {
		return children;
	}

	public void setChildren(List<Code> children) {
		this.children = children;
	}
}
