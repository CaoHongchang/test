<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>
<%@ include file="/system/common.jsp" %>
<html>
<body>
<form id="fm2" method="post">
     <table class="table2">
	     <tr>
		      <td class="left">分区名称</td>
		      <td ><input  name="areano" id="areano"  class="easyui-textbox easyui-validatebox"
		       data-options="required:true,validType:['length[0,4]']" 
		      size="30" style="height:22px"></td>
		  </tr>
		  <tr style="height:50px;line-height:50px;">
		      <td class="left">描述</td>
		      <td >
		      		<input type="text" class="easyui-textbox" name="description" maxlength="100" 
		      		data-options="validType:['length[0,100]'],multiline:true" 
		      		style="width:206px;height:50px;"/>		
		      </td>
		     </tr>
		     <tr style="height:60px;">
		      <td class="left">展位数</td>
		      <td ><input type="text" class="easyui-numberbox"
		       data-options="required:true,min:0,max:499" 
		      id ="position_count" name="positioncount"  size="30" style="height:22px">
		      <div style="color:red;">*只能输入499以下的数值</font>
		      </div>
		  </tr>
   	</table>  
</form> 
</body>
</html>
