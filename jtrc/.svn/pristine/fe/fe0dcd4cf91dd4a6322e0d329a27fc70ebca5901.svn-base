<%@ page language="java" import="java.util.*" pageEncoding="UTF-8" %>

<html>
<head>
   <title>新增快递单号</title>
</head>

<body>
	<form id="fmAdd" method="post">
	    <input type="hidden" name="id" id="id">
		<div>
			<table class="table2">
				<tr>
					<td class="left">
					           快递公司
					</td>
					<td class="right">
						<input  name="kCompany" id="kCompany" class="easyui-textbox" size="20"
							data-options="required:true" style="height: 22px">
					</td>
				</tr>
				<tr>
					<td class="left">
						快递单号
					</td>
					<td class="right">
						<input id="kpno" name="kpno" class="easyui-textbox" size="20"
							data-options="required:true" style="height: 22px">
					</td>
				</tr>
				<tr>
					<td class="left">
						寄出时间
					</td>
					<td class="right">
						<input name="kDate" id="kDate" class="easyui-datetimebox" size="20"
							data-options="required:true" style="height: 22px">
					</td>
				</tr>
			</table>
		</div>
	</form> 
</body>

</html>

